### OpenAI credentials
OPENAI_API_KEY_SANDBOX=sk-your-sandbox-key
OPENAI_API_KEY_STAGING=sk-your-staging-key
OPENAI_API_KEY_PRODUCTION=sk-your-production-key

### Active environment (sandbox | staging | production)
OPENAI_ENVIRONMENT=sandbox

### Model selections (override per deployment as needed)
OPENAI_MODEL_REASONING=gpt-5
OPENAI_MODEL_REASONING_EFFORT=high
OPENAI_MODEL_CODE=gpt-5-codex
OPENAI_MODEL_REALTIME=gpt-realtime
OPENAI_MODEL_REALTIME_MINI=gpt-realtime-mini
OPENAI_MODEL_EMBEDDING=text-embedding-3-small

### Logging / telemetry controls
LOG_LEVEL=INFO
LOG_SAMPLE_PROMPTS=true
LOG_SAMPLE_RESPONSES=false

### ReasoningBank settings
REASONING_BANK_ENABLED=true
REASONING_BANK_PATH=docs/reasoning_bank.json
REASONING_BANK_MAX_ITEMS=500
